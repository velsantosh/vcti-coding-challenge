<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE jasperReport PUBLIC "//JasperReports//DTD Report Design//EN"
   "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">

<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports
   http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="freport" pageWidth="1500" pageHeight="5000"
              columnWidth="300" leftMargin="20" rightMargin="20"
              topMargin="20" bottomMargin="20">

<style name="field" fontSize="11" />
<style name="textRight" style="field" hAlign="Right" />

<subDataset name="dataset1">
    <field name="problem" class="java.lang.String"/>
    <field name="option" class="java.lang.String"/>
    <field name="slectedAnswer" class="java.lang.String"/>
    <field name="correctAnswer" class="java.lang.String"/>
</subDataset>

<subDataset name="dataset2">
    <field name="program" class="java.lang.String"/>
    <field name="ansSubmitted" class="java.lang.String"/>
    <field name="consolidatedOutput" class="java.lang.String"/>
</subDataset>

<parameter name="datasource1" class="java.util.List"/>
<parameter name="datasource2" class="java.util.List"/>

<detail>
    <band height="2000">
    
         <staticText>
					<reportElement mode="Opaque" x="50" y="30" width="300" height="30" />
					<textElement>
						<font size="12"/>
					</textElement>
					<text><![CDATA[Objective Question Summary]]></text>
				</staticText>
				
        <componentElement>
            <reportElement x="30" y="40" width="400" height="200"/>
            <jr:list xmlns:jr="http://jasperreports.sourceforge.net/jasperreports/components"
                        xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports/components
                                            http://jasperreports.sourceforge.net/xsd/components.xsd">

                <datasetRun subDataset="dataset1">
                    <dataSourceExpression><![CDATA[new net.sf.jasperreports.engine.data.JRBeanCollectionDataSource($P{datasource2})]]></dataSourceExpression>
                </datasetRun>
                  
                <jr:listContents height="200" width="400">
                
                <staticText>
					<reportElement mode="Opaque" x="30" y="40" width="100" height="30" />
					<textElement>
						<font size="12"/>
					</textElement>
					<text><![CDATA[Question-]]></text>
				</staticText>
                    <textField>
                        <reportElement x="100" y="40" width="300" height="30" style="field" />

                        <textElement />

                        <textFieldExpression class="java.lang.String"><![CDATA[$F{problem}]]></textFieldExpression>
                    </textField>
                      <staticText>
					<reportElement mode="Opaque" x="30" y="70" width="100" height="30" />
					<textElement>
						<font size="12"/>
					</textElement>
					<text><![CDATA[OPTION-]]></text>
				</staticText>
                    <textField>
                        <reportElement x="100" y="70" width="100" height="30" style="field" />

                        <textElement />

                        <textFieldExpression class="java.lang.String"><![CDATA[$F{option}]]></textFieldExpression>
                    </textField>
                   <staticText>
					<reportElement mode="Opaque" x="30" y="100" width="100" height="30" />
					<textElement>
						<font size="12"/>
					</textElement>
					<text><![CDATA[AnswerSelected-]]></text>
				</staticText>
                    <textField>
                        <reportElement x="100" y="100" width="100" height="30" style="textRight" />
                        <textElement />

                        <textFieldExpression class="java.lang.String"><![CDATA[$F{slectedAnswer}]]></textFieldExpression>
                    </textField>
                    <staticText>
					<reportElement mode="Opaque" x="30" y="130" width="100" height="30" />
					<textElement>
						<font size="12"/>
					</textElement>
					<text><![CDATA[CorrectAnswer-]]></text>
				</staticText>
                   <textField>
                        <reportElement x="100" y="130" width="100" height="30" style="textRight" />
                        <textElement />

                        <textFieldExpression class="java.lang.String"><![CDATA[$F{correctAnswer}]]></textFieldExpression>
                    </textField>
                   
                </jr:listContents>
            </jr:list>
        </componentElement>
                 <staticText>
					<reportElement mode="Opaque" x="800" y="30" width="200" height="30" />
					<textElement>
						<font size="12"/>
					</textElement>
					<text><![CDATA[Subjective Question Summary]]></text>
				</staticText>
        <componentElement>
            <reportElement x="400" y="30" width="1000" height="500"/>
            <jr:list xmlns:jr="http://jasperreports.sourceforge.net/jasperreports/components"
                        xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports/components
                                            http://jasperreports.sourceforge.net/xsd/components.xsd">

                <datasetRun subDataset="dataset2">
                    <dataSourceExpression><![CDATA[new net.sf.jasperreports.engine.data.JRBeanCollectionDataSource($P{datasource1})]]></dataSourceExpression>
                </datasetRun>

                <jr:listContents height="500" width="1000">
                <staticText>
					<reportElement mode="Opaque" x="410" y="30" width="70" height="30" />
					<textElement>
						<font size="12"/>
					</textElement>
					<text><![CDATA[Problem-]]></text>
				</staticText>
                    <textField>
                        <reportElement x="480" y="30" width="300" height="30" style="field" />
                        <textFieldExpression class="java.lang.String"><![CDATA[$F{program}]]></textFieldExpression>
                    </textField>
                     <staticText>
					<reportElement mode="Opaque" x="410" y="60" width="70" height="30" />
					<textElement>
						<font size="12"/>
					</textElement>
					<text><![CDATA[Solution-]]></text>
				</staticText>
                    <textField>
                        <reportElement x="480" y="60" width="300" height="30" style="field" />

                        <textElement />

                        <textFieldExpression class="java.lang.String"><![CDATA[$F{ansSubmitted}]]></textFieldExpression>
                    </textField>
                     <staticText>
					<reportElement mode="Opaque" x="410" y="90" width="50" height="30" />
					<textElement>
						<font size="12"/>
					</textElement>
					<text><![CDATA[Testcase-]]></text>
				</staticText>
                    <textField>
                        <reportElement x="480" y="90" width="100" height="30" style="textRight" />
                        <textElement />

                        <textFieldExpression class="java.lang.String"><![CDATA[$F{consolidatedOutput}]]></textFieldExpression>
                    </textField>

                </jr:listContents>
            </jr:list>
        </componentElement>

    </band>
</detail>

</jasperReport>